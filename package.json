{
  "main": "index.js",
  "name": "Happyo",
  "scripts": {
    "pods": "cd ios/ ; pod install ; cd ..",
    "android": "react-native run-android",
    "gradlewClean": "cd android && ./gradlew clean ;cd ..",
    "buildApk": "cd android && ./gradlew assembleRelease; cd ..",
    "iosBuild": "react-native bundle --entry-file index.js --platform ios --dev false --bundle-output ios/main.jsbundle --assets-dest ios",
    "ios": "react-native run-ios",
    "ios5": "react-native run-ios --simulator='iPhone 5s'",
    "ios6": "react-native run-ios --simulator='iPhone 6'",
    "iosP": "react-native run-ios --simulator='iPhone 8 Plus'",
    "iosMax": "react-native run-ios --simulator='iPhone XS Max'",
    "ios12": "react-native run-ios --simulator='iPhone 12 Pro'",
    "reinstall": "rm -rf node_modules ; yarn",
    "fullReinstall": "yarn reinstall; yarn pods",
    "prettier": "prettier --write '**/*.js'; prettier --write '**/*.ts'",
    "web": "expo start --web",
    "start": "react-native start",
    "codepushProd": "appcenter codepush release-react -a admin-soberman.me/Happy -d Prod",
    "codepushDev": "appcenter codepush release-react -a admin-soberman.me/Happy -d Dev",
    "cleanStart": "yarn reinstall; yarn start",
    "test": "jest",
    "test:tslint": "tsc --noemit",
    "test:eslint": "eslint --fix --ignore-path .gitignore ./src --ext .ts,.tsx",
    "test:lint": "run-s test:tslint test:eslint"
  },
